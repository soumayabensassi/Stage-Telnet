                                {% for d in domaine %}
                                <li>
                                    <a href="{{ path('showabonnementparclient',{'cat': d.nom,'id': user.id}) }}">{{ d.nom}}</a></li>
                                {% endfor %}
                            </ul>
                                </li>
                                <li><a href="{{ path('profil') }}"> Profil </a>


                                </li>



                                <li><a > <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bell-fill" viewBox="0 0 16 16">
